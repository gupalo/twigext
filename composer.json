{
    "name": "gupalo/twigext",
    "description": "Twig Extensions",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "gupalo/arrayutils": "^1.9",
        "gupalo/dateutils": "^1.13",
        "gupalo/json": "^1.0",
        "symfony/property-access": "^5.4|^6.0",
        "twig/twig": "^3.5"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.0.2"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\TwigExt\\": "src/TwigExt/",
            "Gupalo\\TwigExtException\\": "src/TwigExtException"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\Tests\\TwigExt\\": "tests/TwigExt/"
        }
    }
}
